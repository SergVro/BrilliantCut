<?xml version="1.0" encoding="utf-8" ?>
<module productName="EPiTubefacetFilter" loadFromBin="false" clientResourceRelativePath=""
        type="EPiTube.facetFilter.Widget.Edit.EPiTubefacetFilterModule, EPiTube.facetFilter.Widget">
  <assemblies>
    <add assembly="EPiTube.facetFilter.Widget" />
  </assemblies>

  <routes>
    <route controllerPrefix="EPi" url="Commerce/{controller}/{action}">
      <defaults>
        <add key="moduleArea" value="EPiServer.Commerce.Shell" />
        <add key="controller" value="" />
        <add key="action" value="Index" />
        <add key="id" value="" />
      </defaults>
    </route>
    <route url="{moduleArea}/{controller}/{action}/"></route>
  </routes>

  <dojo>
    <packages>
      <add name="epitubefacetfilter" location="Scripts" />
    </packages>
  </dojo>

  <clientModule initializer="epitubefacetfilter.EPiTubefacetFilterModule">
    <moduleDependencies>
      <add dependency="Commerce" />
    </moduleDependencies>
    <requiredResources>
      <add name="commerce.ui" />
    </requiredResources>
  </clientModule>
</module>